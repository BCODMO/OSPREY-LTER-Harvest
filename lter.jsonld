    {
      "@id": "dataone",
      "@type": "SoftwareApplication",
      "name": "DataONE SOLR Endpoint",
      "provider": { "@id": "https://dataone.org"},
      "potentialAction": {
        "@id": "dataone-solr-search-v1",
        "@type": "SearchAction",
        "target": {
          "@id": "dataone-solr-search-v1-target",
          "@type": "EntryPoint",
          "contentType": "application/xml",
          "urlTemplate": "https://cn.dataone.org/cn/v1/query/solr/?fl={fl}&q={q}&sort={sort}&rows={rows}&start={start}"
        },
        "query-input": [
          {
            "@type": "PropertyValueSpecification",
            "valueRequired": true,
            "readonlyValue": true,
            "valueName": "fl",
            "defaultValue": "id,title,resourceMap"
          },
          {
            "@type": "PropertyValueSpecification",
            "valueRequired": true,
            "readonlyValue": true,
            "valueName": "sort",
            "defaultValue": "id%20asc"
          },
          {
            "@type": "PropertyValueSpecification",
            "valueRequired": true,
            "valueName": "q"
          },
          {
            "@type": "PropertyValueSpecification",
            "valueRequired": true,
            "valueName": "rows",
            "defaultValue": 50
          },
          {
            "@type": "PropertyValueSpecification",
            "valueRequired": true,
            "valueName": "start",
            "defaultValue": 0
          }
        ]
      }
    },
    {
      "@id": "pasta-eml",
      "@type": "SoftwareApplication",
      "name": "EDI PASTA EML Search API",
      "provider": {
        "@type": "Organization",
        "name": "Environmental Data Initiative",
        "url": "https://environmentaldatainitiative.org/",
        "logo": {
          "@type": "ImageObject",
          "url": "https://environmentaldatainitiative.files.wordpress.com/2017/04/cropped-edi-logo-svg.png"
        }
      },
      "potentialAction": {
        "@id": "pasta-eml-search",
        "@type": "SearchAction",
        "target": {
          "@id": "pasta-eml-search-target",
          "@type": "EntryPoint",
          "contentType": "application/xml",
          "urlTemplate": "http://pasta.lternet.edu/package/search/eml?fl=doi,title&sort=score,desc&sort=packageid,asc&debug=false&' . $lter_site_info['query'] . '&rows=' . $count . '&start=' . $start;"
        },
        "result": {
          "@type": "Dataset",
          "name-output": { 
            "@type": "PropertyValueSpecification", 
            "valueRequired": true,
            "valueName": "title" 
          },
          "url-output": { 
            "@type": "PropertyValueSpecification", 
            "valueRequired": true,
            "valueName": "doi" 
          }
        },
        "query-input": [
          {
            "@type": "PropertyValueSpecification",
            "valueRequired": true,
            "readonlyValue": true,
            "valueName": "fl",
            "defaultValue": "doi,title"
          },
          {
            "@type": "PropertyValueSpecification",
            "description": "sorted by the 'packageid' field ascending to ensure that the most recent dataset record is read last",
            "valueRequired": true,
            "readonlyValue": true,
            "multipleValues": true,
            "valueName": "sort",
            "defaultValue": ["score,desc", "packageid,asc"]
          },
          {
            "@type": "PropertyValueSpecification",
            "valueRequired": true,
            "readonlyValue": true,
            "valueName": "debug",
            "defaultValue": "false"
          },
          {
            "@type": "PropertyValueSpecification",
            "valueName": "fq"
          },
          {
            "@type": "PropertyValueSpecification",
            "valueName": "q"
          },
          {
            "@type": "PropertyValueSpecification",
            "valueName": "defType"
          },
          {
            "@type": "PropertyValueSpecification",
            "valueRequired": true,
            "valueName": "rows",
            "defaultValue": 50
          },
          {
            "@type": "PropertyValueSpecification",
            "valueRequired": true,
            "valueName": "start",
            "defaultValue": 0
          }
        ]
      }
    },
